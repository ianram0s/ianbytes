{
	"name": "ianbytes",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"pages:dev": "wrangler pages dev --compatibility-date=2023-12-18 --proxy 5173 -- yarn dev",
		"pages:deploy": "yarn build && wrangler pages deploy .svelte-kit/cloudflare",
		"generate": "drizzle-kit generate:sqlite --schema=./src/lib/server/db/schema.ts"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20231218.0",
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"drizzle-kit": "^0.20.10",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3",
		"wrangler": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-sqlite": "^2.0.1",
		"@lucia-auth/oauth": "^3.5.1",
		"bits-ui": "^0.13.5",
		"clsx": "^2.1.0",
		"drizzle-orm": "^0.29.3",
		"formsnap": "^0.4.2",
		"lucia": "^2.7.6",
		"lucide-svelte": "^0.303.0",
		"mode-watcher": "^0.1.2",
		"sveltekit-superforms": "^1.13.2",
		"tailwind-merge": "^2.2.0",
		"tailwind-variants": "^0.1.20",
		"zod": "^3.22.4"
	}
}
